{
    "name": "juizmill/list-service",
    "description": "Simple Help Desk System",
    "license": "BSD-3-Clause",
    "keywords": [
        "framework",
        "zf2",
        "php",
        "orm",
        "doctrine"
    ],
    "authors": [
        {
            "name": "Jesus Vieira",
            "email": "jesusvieiradelima@gmail.com"
        }
    ],
    "homepage": "http://framework.zend.com/",
    "require": {
        "php": ">=5.4",
        "zendframework/zendframework": "2.3.*",
        "doctrine/common": ">=2.1",
        "doctrine/doctrine-orm-module": "0.9.*",
        "doctrine/data-fixtures": "1.0.*@dev",
        "zf-commons/zfc-user-doctrine-orm": "1.0.0",
        "bjyoungblood/bjy-authorize": "1.5.*",
        "gedmo/doctrine-extensions": "2.3.*",
        "hounddog/doctrine-data-fixture-module": "0.0.*",
        "doctrine/migrations": "~1.0@dev",
        "acelaya/zf2-acmailer": "~4.0",
        "symfony/filesystem": "~3.0@dev"
    },
    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "zendframework/zend-developer-tools": "dev-master"
    },
    "config": {
        "bin-dir": "bin",
        "process-timeout": 9000
    },
    "scripts": {
        "post-update-cmd": "php setting.php",
        "post-install-cmd": "php setting.php"
    }
}
