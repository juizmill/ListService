{
    "name": "juizmill/list-service",
    "description": "Simple Help Desk System",
    "license": "BSD-3-Clause",
    "keywords": [
        "framework",
        "zf2",
        "php",
        "orm",
        "doctrine"
    ],
    "authors": [
        {
            "name": "Jesus Vieira",
            "email": "jesusvieiradelima@gmail.com"
        }
    ],
    "homepage": "http://framework.zend.com/",
    "require": {
        "php": ">=5.4",
        "zendframework/zendframework": "2.3.*",
        "doctrine/common": ">=2.1",
        "doctrine/doctrine-orm-module": "dev-master",
        "hounddog/doctrine-data-fixture-module": "dev-master",
        "zf-commons/zfc-user-doctrine-orm": "0.1.*@dev",
        "bjyoungblood/bjy-authorize": "1.5.*@dev",
        "gedmo/doctrine-extensions": "dev-master"
    },
    "minimum-stability": "dev",
    "require-dev": {
        "squizlabs/php_codesniffer": "2.*",
        "phpmd/phpmd" : "@stable",
        "fabpot/php-cs-fixer": "dev-master",
        "phpunit/phpunit": "3.7.*",
        "satooshi/php-coveralls": "dev-master",
        "zendframework/zend-developer-tools": "dev-master"
    },
    "config": {
        "bin-dir": "bin",
        "process-timeout": 9000
    },
    "scripts": {
        "post-update-cmd": "php setting.php",
        "post-install-cmd": "php setting.php"
    }
}
